{
	"typescript": {
		"indentWidth": 4,
		"useTabs": true
	},
	"json": {
		"indentWidth": 4,
		"useTabs": true
	},
	"markdown": {
	},
	"includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md}"],
	"excludes": [
		"**/node_modules",
		"**/*-lock.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.68.2.wasm",
		"https://plugins.dprint.dev/json-0.15.2.wasm",
		"https://plugins.dprint.dev/markdown-0.13.2.wasm"
	]
}
